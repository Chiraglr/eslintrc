{
  "root": true,
  "plugins": [
      // "eslint-plugin",
      "@babel",
      "react",
      "jsx",
      "prefer-optional-chaining"
  ],
  "extends": [
      // "plugin:eslint-plugin/recommended"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "allowImportExportEverywhere": true,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "requireConfigFile": false,
    "babelOptions": {
      "presets": ["@babel/preset-react"]
    }
  },
  "env": {
    "browser": true,
    "es2021": true
  },
  "rules": {
    // Override our default settings just for this directory
    "constructor-super": 1,
    "no-async-promise-executor": 1,
    "no-compare-neg-zero": 1,
    "no-const-assign": 1,
    "no-constructor-return": 1,
    "no-dupe-args": 1,
    "no-dupe-class-members": 1,
    "no-dupe-else-if": 1,
    "no-dupe-keys": 1,
    "no-duplicate-imports": 1,
    "no-empty-pattern": 1,
    "no-ex-assign": 1,
    "no-func-assign": 1,
    "no-import-assign": 1,
    "no-irregular-whitespace": 1,
    "no-self-assign": 1,
    "no-sparse-arrays": 1,
    "no-template-curly-in-string": 1,
    "no-this-before-super": 1,
    "no-unexpected-multiline": 1,
    "no-unreachable": 1,
    "no-unreachable-loop": 1,
    "no-unsafe-negation": 1,
    "no-unsafe-optional-chaining": 1,
    // "no-unused-private-class-members": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "valid-typeof": 1,
    "camelcase": 1,
    "default-case-last": 1,
    "default-param-last": 1,
    "dot-notation": 1,
    "eqeqeq": 1,
    "no-caller": 1,
    "no-eval": 1,
    "no-var": 1,
    "quote-props": [1, "as-needed"],
    "prefer-const": 1,
    "prefer-destructuring": 1,
    "prefer-object-spread": 1,
    "prefer-rest-params": 1,
    "prefer-spread": 1,
    "prefer-template": 1,
    "require-await": 1,
    "array-bracket-spacing": 1,
    "no-extra-parens": 1,
    "no-trailing-spaces": 1,
    "no-whitespace-before-property": 1,
    "indent": [1, 2],
    "jsx-quotes": 1,
    "key-spacing": 1,
    "max-len": [1, { "code": 138 }],
    "max-statements-per-line": 1,
    "comma-spacing": 1,
    "comma-style": [1, "last"],
    "computed-property-spacing": 1,
    "comma-dangle": 1,
    "arrow-parens": 1,
    "semi": 1,
    "semi-style": 1,
    "switch-colon-spacing": 1,
    // "prefer-optional-chaining": 1,
    "react/destructuring-assignment": 1,
    "react/display-name": 1,
    // "react/hook-use-state": 1,
    "react/no-adjacent-inline-elements": 1,
    "react/no-array-index-key": 1,
    "react/no-deprecated": 1,
    "react/no-did-mount-set-state": 1,
    "react/no-direct-mutation-state": 1,
    // "react/no-unused-class-component-methods": 1,
    "react/no-unused-state": 1,
    "react/style-prop-object": 1,
    "react/jsx-boolean-value": 1,
    // "react/jsx-closing-bracket-location": [1, "line-aligned"],
    "react/jsx-curly-brace-presence": [1, { "props": "never", "children": "never" }],
    "react/jsx-curly-newline": 1,
    "react/jsx-curly-spacing": [1, {"children": true}],
    "react/jsx-equals-spacing": 1,
    "react/jsx-fragments": 1,
    "react/jsx-indent": [1, 2, {"checkAttributes": true, "indentLogicalExpressions": true}],
    "react/jsx-indent-props": [1, { "indentMode": 2, "ignoreTernaryOperator": true}],
    "react/jsx-key": 1,
    "react/jsx-props-no-multi-spaces": 1,
    "react/jsx-no-duplicate-props": 1,
    "react/jsx-no-target-blank": 1,
    // "react/jsx-space-before-closing": 1,
    // "react/jsx-one-expression-per-line": [1, {"allow": "single-child"}],
    "react/jsx-tag-spacing": 1,
    "react/jsx-uses-vars": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-no-undef": 1,
    "react/jsx-handler-names": [1, {
      "eventHandlerPrefix": "on",
      "eventHandlerPropPrefix": "on",
      "checkLocalVariables": true,
      "checkInlineFunction": true
    }]
  }
}
